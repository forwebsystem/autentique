{
    "name": "forwebsystem/autentique",
    "description": ":SDK para comunicação para autenticação com o serviço de assinatura de documentos digitalmente",
    "license": "MIT",
    "authors": [
        {
            "name": "Joao Paulo P Villela",
            "email": "joaopaulo@forwebsystem.com.br",
            "homepage": "http://forwebsystem.com.br"
        }
    ],
    "homepage": "https://github.com/forwebsystem/autentique",
    "keywords": ["Laravel", "Autentique", "Assinatura digital"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "ForWebSystem\\Autentique\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ForWebSystem\\Autentique\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ForWebSystem\\Autentique\\AutentiqueServiceProvider"
            ],
            "aliases": {
                "Autentique": "ForWebSystem\\Autentique\\Facades\\Autentique"
            }
        }
    }
}
